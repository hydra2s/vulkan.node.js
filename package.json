{
  "name": "vulkan.node.js",
  "version": "1.3.235.3",
  "description": "Implement Vulkan API bridge for node.js",
  "main": "index.js",
  "scripts": {
    "triangle": "cd examples/triangle && node index.js",
    "triangle-rtx": "cd examples/triangle-rtx && node index.js",
    "test": "cd examples/init-test && node index.js",
    "install": "cd src && node main.js && cd ../ && cmake-js -r node --debug"
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "19.1.0",
    "arch": "x64"
  },
  "keywords": [
    "vulkan",
    "api",
    "bridge",
    "vulkan.node.js",
    "raw",
    "vulkan-api",
    "binding",
    "bindings",
    "node",
    "js",
    "javascript",
    "native",
    "khronos"
  ],
  "author": "hydra2s",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.10",
    "@petamoriken/float16": "^3.6.6",
    "@rgrove/parse-xml": "^4.0.1",
    "bindings": "^1.5.0",
    "gl-matrix": "^3.4.3",
    "json-6": "^1.1.4",
    "json5": "^2.2.1",
    "jsox": "^1.2.116",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.3.0",
    "win32-api": "^20.1.0"
  },
  "devDependencies": {
    "assemblyscript": "^0.24.0",
    "cmake-js": "^7.0.0"
  },
  "type": "module"
}
