{
  "name": "vulkan.node.js",
  "version": "1.0.0",
  "description": "Implement Vulkan API bridge for node.js",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "test2": "electron main.cjs",
    "install": "cd src && node main.js && cd ../ && cmake-js -r node --debug",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve ."
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "19.1.0",
    "arch": "x64"
  },
  "keywords": [
    "vulkan",
    "api",
    "bridge"
  ],
  "author": "hydra2s",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@mapbox/node-pre-gyp": "^1.0.10",
    "@rgrove/parse-xml": "^4.0.1",
    "bindings": "^1.5.0",
    "electron-esm": "^1.0.1",
    "gl-matrix": "^3.4.3",
    "glfw-raub": "^4.6.2",
    "json-6": "^1.1.4",
    "json5": "^2.2.1",
    "jsox": "^1.2.116",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.3.0",
    "win32-api": "^20.1.0"
  },
  "devDependencies": {
    "assemblyscript": "^0.24.0",
    "cmake-js": "^7.0.0",
    "electron-nightly": "^23.0.0-nightly.20221118"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}
