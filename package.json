{
  "name": "node-vulkan-api",
  "version": "1.0.0",
  "description": "Implement Vulkan API bridge for node.js",
  "main": "index.js",
  "scripts": {
    "test": "cd src && node test.js && cd ../",
    "install": "cd src && node main.js && cd ../ && cmake-js -r node --debug",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve ."
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "19.1.0",
    "arch": "x64"
  },
  "keywords": [
    "vulkan",
    "api",
    "bridge"
  ],
  "author": "hydra2s",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.10",
    "@rgrove/parse-xml": "^4.0.1",
    "asciidoctor": "^2.2.6",
    "bindings": "^1.5.0",
    "c-struct": "^0.0.5",
    "fast-xml-parser": "^4.0.11",
    "glfw-raub": "^4.6.2",
    "json-6": "^1.1.4",
    "json5": "^2.2.1",
    "jsox": "^1.2.116",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.3.0",
    "parse-xml": "^1.0.1",
    "restructure": "^3.0.0",
    "shared-structs": "^1.4.1",
    "struct-buffer": "^5.2.0",
    "typed-struct": "^2.3.0"
  },
  "devDependencies": {
    "assemblyscript": "^0.24.0",
    "cmake-js": "^7.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}
